<!DOCTYPE html> <html lang="en"> <head>     <meta charset="UTF-8">     <title>Working Proxy Solution</title>     <style>         body { font-family: Arial; max-width: 800px; margin: 0 auto; padding: 20px; }         #proxy-frame { width: 100%; height: 70vh; border: 1px solid #ddd; margin-top: 20px; }         .controller { display: flex; gap: 10px; margin: 15px 0; }         button { padding: 8px 15px; background: #4285f4; color: white; border: none; border-radius: 4px; }     </style> </head> <body>     <h1>Fixed Universal Proxy</h1>     <input type="url" id="target-url" placeholder="https://example.com" style="width: 70%; padding: 8px;">     <div class="controller">         <button id="fetch-btn">Load</button>         <button id="direct-btn">Direct Load</button>     </div>     <div id="status"></div>     <iframe id="proxy-frame"></iframe>      <script>         const proxies = [             'https://corsproxy.org/?',             'https://api.allorigins.win/get?url=',             'https://thingproxy.freeboard.io/fetch/'         ];          document.getElementById('fetch-btn').addEventListener('click', async () => {             const url = document.getElementById('target-url').value.trim();             if (!url) return;                          document.getElementById('status').textContent = 'Connecting...';                          for (const proxy of proxies) {                 try {                     const proxyUrl = proxy.includes('allorigins.win')                          ? `${proxy}${encodeURIComponent(url)}`                         : `${proxy}${url}`;                                          const response = await fetch(proxyUrl);                     const data = proxy.includes('allorigins.win')                          ? await response.json().then(d => d.contents)                         : await response.text();                                          document.getElementById('proxy-frame').srcdoc = data;                     document.getElementById('status').textContent = 'Loaded successfully!';                     return;                 } catch (e) {                     console.log(`Proxy ${proxy} failed`);                 }             }             document.getElementById('status').textContent = 'All proxies failed';         });          document.getElementById('direct-btn').addEventListener('click', () => {             const url = document.getElementById('target-url').value.trim();             if (!url) return;             document.getElementById('proxy-frame').src = url;             document.getElementById('status').textContent = 'Loading directly (may be blocked)...';         });     </script> </body> 
</html> 
